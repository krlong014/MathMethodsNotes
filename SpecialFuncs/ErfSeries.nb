   RowBox[{"2", " ", 
    SuperscriptBox["x", 
     RowBox[{
      RowBox[{"2", " ", "n"}], "+", "1"}]]}], 
   RowBox[{
    SqrtBox["\[Pi]"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", " ", "n"}], "+", "1"}], ")"}], " ", 
    RowBox[{"n", "!"}]}]], TraditionalForm]], "Output",
