Cell[CellGroupData[{
Cell["Accuracy of series approximations to the error function", "Title",
 CellChangeTimes->{{3.855005063643464*^9, 
  3.855005105004345*^9}},ExpressionUUID->"d891a97e-b625-4d70-82c2-\
0a6e7177dd78"],

Cell["\<\
Katharine Long
Texas Tech University\
\>", "Subtitle",
 CellChangeTimes->{{3.855005112212574*^9, 
  3.855005122837151*^9}},ExpressionUUID->"f433edb6-fd29-4080-be4a-\
9a4c70ab44cb"],

Cell["\<\
Here we compare two series approximations to the error function\
\>", "Text",
 CellChangeTimes->{{3.8550054443686113`*^9, 
  3.8550054605367517`*^9}},ExpressionUUID->"37a35ccc-f22a-408a-a33b-\
65e12f89c5aa"],

Cell[CellGroupData[{

Cell[TextData[{
 "The Maclaurin series ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"erf", "(", "x", ")"}], "=", 
    RowBox[{
     FractionBox["2", 
      SqrtBox["\[Pi]"]], 
     SuperscriptBox[
      RowBox[{
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"n", "=", "0"}], "\[Infinity]"], "(", 
       RowBox[{"-", "1"}], ")"}], "n"], 
     FractionBox[
      SuperscriptBox["x", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "1"}]], 
      RowBox[{
       RowBox[{"n", "!"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "n"}], "+", "1"}], ")"}]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "19953930-df67-46b9-840c-a4bded21f005"],
 " which is ",
 StyleBox["convergent",
  FontSlant->"Italic"],
 " for all ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "78c7b053-47bf-48ae-a9e1-47da0a9522e1"],
 " but ",
 StyleBox["useful",
  FontSlant->"Italic"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", "x", "\[RightBracketingBar]"}], 
    "\[LessTilde]", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d70dc9fe-23b0-4687-a329-954a2cba3162"],
 "."
}], "Item",
 CellChangeTimes->{{3.855005471537088*^9, 3.855005477513226*^9}, {
  3.8550056481411133`*^9, 3.855005679287197*^9}, {3.855005716759036*^9, 
  3.8550057299740133`*^9}, {3.855005766946569*^9, 3.855005779606862*^9}, {
  3.855006197424244*^9, 3.855006224457005*^9}, {3.8550071015991173`*^9, 
  3.855007101799409*^9}},ExpressionUUID->"494e6d3d-9078-454e-b80d-\
361c7bcd5690"],

Cell[TextData[{
 "The asymptotic series ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"erf", "(", "x", ")"}], "\[Tilde]", " ", 
    RowBox[{"1", " ", "-", " ", 
     RowBox[{
      FractionBox[
       SuperscriptBox["e", 
        RowBox[{"-", 
         SuperscriptBox["x", "2"]}]], 
       RowBox[{
        SqrtBox["\[Pi]"], "x"}]], "[", 
      RowBox[{"1", "+", 
       RowBox[{
        SuperscriptBox[
         RowBox[{
          UnderscriptBox[
           OverscriptBox["\[CapitalSigma]", "\[Infinity]"], 
           RowBox[{"n", "=", "1"}]], "(", 
          RowBox[{"-", "1"}], ")"}], "n"], 
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "n"}], "-", "1"}], ")"}], "!!"}], 
         RowBox[{
          SuperscriptBox["2", "n"], 
          SuperscriptBox["x", 
           RowBox[{"2", "n"}]]}]]}]}], "]"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "86c758c9-2f38-4f98-a0fb-e36794a342ca"],
 " which is ",
 StyleBox["divergent",
  FontSlant->"Italic"],
 " for all ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "07d9a817-201c-491d-9f9b-d9319dac7bc5"],
 " but is ",
 StyleBox["useful ",
  FontSlant->"Italic"],
 "for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", "x", "\[RightBracketingBar]"}], 
    "\[GreaterTilde]", 
    RowBox[{"3", "-", "4"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9b9bcb93-7db2-4ec9-8404-38ca8b7c7029"],
 "."
}], "Item",
 CellChangeTimes->{{3.855005471537088*^9, 3.855005477513226*^9}, {
  3.8550056481411133`*^9, 3.855005679287197*^9}, {3.855005716759036*^9, 
  3.8550057299740133`*^9}, {3.8550057819025*^9, 3.8550058108560123`*^9}, {
  3.8550058679291067`*^9, 3.855005888994021*^9}, {3.855005935877483*^9, 
  3.855005948818242*^9}, {3.8550059937160597`*^9, 3.855005995276937*^9}, {
  3.855006096606629*^9, 3.855006179112276*^9}, {3.8550062499848948`*^9, 
  3.85500628768443*^9}, {3.855006335875998*^9, 
  3.85500633752264*^9}},ExpressionUUID->"61dae1ba-b4fa-40d6-8687-\
d8bcee6ef2e9"]
}, Open  ]],

Cell["\<\
Note: For high-accuracy calculations, neither of these methods should be used \
as-is. An acceleration method such as the Shanks transformation \
(https://en.wikipedia.org/wiki/Shanks_transformation) should be used. \
Alternatively, a error-minimizing method such as a Chebyshev polynomial \
expansion can be used.  \
\>", "Text",
 CellChangeTimes->{{3.8550063682200193`*^9, 3.85500645988577*^9}, {
  3.8550064913586607`*^9, 3.8550064997429743`*^9}, {3.855006535384136*^9, 
  3.855006557629998*^9}, {3.8550065901849747`*^9, 3.855006664570586*^9}, {
  3.8550066998449907`*^9, 3.855006701900222*^9}, {3.8550072554435577`*^9, 
  3.855007259969974*^9}},ExpressionUUID->"69a001ac-d362-4f10-84da-\
7fb7955ec972"],

Cell[CellGroupData[{

Cell[TextData[{
 "Error in Maclaurin expansion for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"erf", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "50e5bee3-3911-4ef6-b904-8ff2e0559794"]
}], "Section",
 CellChangeTimes->{{3.855003686242803*^9, 
  3.855003696634811*^9}},ExpressionUUID->"1bf3e0ea-4f87-4557-8edc-\
243017a3223c"],

Cell[TextData[{
 "This series converges for all ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9d640b4c-e057-4c3f-8235-66cce63f114b"],
 ", so in principle can produce accurate results for any ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5babac94-e3f6-4b8f-8a4d-00bfaf8f3695"],
 ". However, for large ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "de93f4d1-8936-4409-ac09-eb9162caa4c3"],
 " it can take very many terms to reach a small error, making it \
unsatisfactory as a practical method for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftBracketingBar]", "x", "\[RightBracketingBar]"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "64200af0-a103-48de-9b7f-83dd03ac2fd5"],
 " larger than about 2. "
}], "Text",
 CellChangeTimes->{{3.855006859656516*^9, 3.8550069169296417`*^9}, {
  3.855007031542139*^9, 
  3.855007139047216*^9}},ExpressionUUID->"61a4eba4-7ad1-4db6-96ae-\
ff94215e8b31"],

Cell[TextData[{
 "The Maclaurin series for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"erf", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "23e49b78-7091-4844-bf17-6e057be4722c"],
 " is a convergent alternating series, so the error after ",
 Cell[BoxData[
  FormBox["N", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "de295536-e999-4943-a797-b5931e156855"],
 " terms is bounded by the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "+", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f8a6f0b3-87a8-4055-b44d-c5d576e0b04b"],
 "-th term. "
}], "Text",
 CellChangeTimes->{{3.855007292426283*^9, 
  3.8550073401557703`*^9}},ExpressionUUID->"1871ed9a-7390-41ba-a90a-\
d7f45be95c38"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   SubscriptBox["e", "n_"], "[", "x_", "]"}], "=", 
  RowBox[{
   RowBox[{"2", "/", 
    RowBox[{"Sqrt", "[", "Pi", "]"}]}], " ", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "^", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "+", "1"}], ")"}]}], "/", 
     RowBox[{"n", "!"}]}], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "n"}], "+", "1"}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.855001163461285*^9, 3.8550011870746717`*^9}},
