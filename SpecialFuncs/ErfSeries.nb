  FormBox[
   RowBox[{"n", "\[Rule]", " ", "\[Infinity]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a81f9164-d721-4a4d-823c-f1baaed782d2"],
